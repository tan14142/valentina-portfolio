{
  "name": "vite-vue-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.47",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.19.0",
    "prettier": "^2.4.1",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vue-tsc": "^1.2.0"
  },
  "eslintConfig": {
    "root": true,
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": false
        }
      ],
      "quotes": [
        "error",
        "double"
      ]
    },
    "space-in-brackets": "always"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "all"
  }
}
